@page "/illustrations"
<h2 class="text-center">Иллюстрации</h2>
<div id="carouselExampleControls" class="container carousel slide w-75" data-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img class="d-block w-100" src="https://potokmedia.ru/wp-content/uploads/2020/03/1-1.png" alt="Первый слайд">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="https://avatars.mds.yandex.net/get-zen_doc/61014/pub_5d9b5d45ec575b00b0155353_5d9b5e03d5bbc300aff944b9/scale_1200" alt="Второй слайд"/>
